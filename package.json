{
  "name": "vf-monorepo",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "author": "Lynn Chen<https://github.com/Jackie1210>",
  "scripts": {
    "preinstall": "node scripts/preinstall",
    "format": "prettier --write .",
    "lint": "eslint packages/*/{src,types}/**/*",
    "clean": "rimraf dist types packages/*/dist",
    "build": "cd packages/vf && pnpm build"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "devDependencies": {
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "vue": "^3.2.20",
    "vite": "^2.6.10",
    "chalk": "^4.1.2",
    "eslint": "^8.0.1",
    "eslint-define-config": "^1.1.1",
    "eslint-plugin-vue": "^7.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3",
    "vue-eslint-parser": "^7.11.0",
    "yorkie": "^2.0.0"
  },
  "packageManager": "pnpm"
}
